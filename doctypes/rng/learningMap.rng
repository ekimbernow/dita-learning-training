            <rngShell>urn:oasis:names:tc:dita:spec:learning:rng:learningMap.rng<var presep=":" name="ditaver"/>
            </rngShell>
            <xsdShell>urn:oasis:names:tc:dita:spec:learning:xsd:learningMap.xsd<var presep=":" name="ditaver"/>
            </xsdShell>
