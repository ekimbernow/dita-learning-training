        </optional>
        <ref name="univ-atts"/>
     </define>
      <define name="lcInstructornote2.element">
        <element name="lcInstructornote" dita:longName="Instructor note">
          <a:documentation><![CDATA[Use the <lcInstructornote2> element to provide information or notes you want to provide to the course instructor. These notes can be conditionnalized out of content you intend to deliver to the learner. ]]></a:documentation>
          <ref name="lcInstructornote2.attlist"/>
          <ref name="lcInstructornote2.content"/>
        </element>
      </define>
      <define name="lcInstructornote2.attlist" combine="interleave">
        <ref name="lcInstructornote2.attributes"/>
      </define>
